{
  "typescript": {
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": "asi",
    "quoteStyle": "auto",
    "trailingCommas": "never"
  },
  "json": {
    "indentWidth": 2
  },
  "includes": ["**/*.{ts,tsx,js,jsx,json}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "dist"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.88.0.wasm",
    "https://plugins.dprint.dev/json-0.17.2.wasm"
  ]
}
