{
	"bin": {
		"wcheck": "./dist/bin/cli.js",
		"wts-check": "./dist/bin/cli.js"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"effect": "^3.19.13",
		"picocolors": "^1.1.1",
		"typescript": "^5.9.3"
	},
	"description": "Comprehensive code quality checker with TypeScript, unused code, dependencies, and more - Built with Effect-TS",
	"devDependencies": {
		"@types/bun": "^1.3.5",
		"@types/node": "^25.0.3",
		"knip": "^5.78.0",
		"tsdown": "^0.18.3",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	},
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"keywords": [
		"cli",
		"checker",
		"type-check",
		"unused-code",
		"dependencies",
		"code-quality",
		"effect-ts",
		"functional",
		"typescript",
		"linter"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"name": "check",
	"scripts": {
		"watch": "bun lint && bun watch && bun test",
		"start": "bun --watch dev && vitest:ui",
		"postinstall": "bunx taze -r -w -i",
		"dev": "bun run src/index.ts",
		"lint": "tsc --noEmit && biome lint --write",
		"build": "tsdown",
		"format": "biome format --write",
		"test": "vitest --run",
		"verify": "bun format && bun lint && bun test && bun audit && run build"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "0.1.0",
	"packageManager": "bun@1.3.5"
}
