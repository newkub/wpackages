{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"moduleDetection": "force",

		"downlevelIteration": true,
		"jsxImportSource": "react",
		"allowSyntheticDefaultImports": true,
		"allowJs": false,
		"isolatedDeclarations": true,
		"declarationMap": true,
		"declaration": true,
		"noEmit": false,
		"types": ["@types/bun", "@types/node"],
		"paths": {
			".": ["./src/types/vnode.ts"]
		}
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"]
}
