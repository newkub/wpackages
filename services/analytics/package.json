{
  "name": "@wpackages/analytics",
  "version": "0.0.1",
  "private": true,
  "description": "Analytics service with Effect-TS for event tracking and batch sending",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "watch": "bun --watch verify",
    "dev": "bun run src/main.ts",
    "format": "dprint fmt",
    "lint": "tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "verify": "bun run format && bun audit && bun run lint && bun run test:coverage && bun run build && bun run dev"
  },
  "dependencies": {
    "effect": "^3.11.5",
    "@effect/schema": "^0.75.5",
    "@wpackages/resilience": "workspace:*",
    "@wpackages/cache": "workspace:*",
    "@wpackages/async": "workspace:*",
    "@wpackages/schema": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "dprint": "^0.49.0",
    "bun-types": "^1.1.43",
    "vitest": "^4.0.16",
    "@vitest/coverage-istanbul": "^4.0.16"
  },
  "keywords": [
    "analytics",
    "event-tracking",
    "effect",
    "functional",
    "typescript"
  ],
  "module": "./dist/index.mjs",
  "packageManager": "bun@1.1.43",
  "engines": {
    "bun": ">=1.1.43"
  },
  "publishConfig": {
    "access": "public"
  }
}
